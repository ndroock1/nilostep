      RowBox[{
       SubsuperscriptBox["\[Integral]", "2", "x"], 
       RowBox[{
        FractionBox[
         RowBox[{"\[Pi]", "(", "t", ")"}], "t"], 
        RowBox[{"\[DifferentialD]", "t"}]}]}]}]}],
    Background->None], TraditionalForm]],ExpressionUUID->
