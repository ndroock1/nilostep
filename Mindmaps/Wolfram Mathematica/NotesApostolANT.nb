Cell[TextData[{
 "The inverse function is defined by the following recursive definitions:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["f", 
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}]], "(", "1", ")"}], "=", 
    SuperscriptBox[
     RowBox[{"f", "(", "1", ")"}], 
     RowBox[{"-", "1"}]]}], TraditionalForm]],ExpressionUUID->
  "d28d8f53-3428-41f6-9ac9-3221c3cd7058"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["f", 
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}]], "(", "n", ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", "1"}], "/", 
      RowBox[{"f", "(", "1", ")"}]}], 
     RowBox[{"(", 
      RowBox[{
       UnderscriptBox["\[Sum]", 
        RowBox[{"d", "<", "n"}]], 
       RowBox[{
        RowBox[{"f", "(", 
         RowBox[{"n", "/", "d"}], ")"}], 
        RowBox[{
         SuperscriptBox["f", 
          RowBox[{"(", 
           RowBox[{"-", "1"}], ")"}]], "(", "d", ")"}]}]}], ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"812345bf-8842-49ef-8af7-43a46eba23e5"]
}], "Text",
