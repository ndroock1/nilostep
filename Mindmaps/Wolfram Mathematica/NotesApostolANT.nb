Cell[TextData[{
 Cell[BoxData[
  RowBox[{
   RowBox[{"\[Zeta]", "[", "s", "]"}], "=", 
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"k", "=", "1"}], "\[Infinity]"], 
     FractionBox["1", 
      SuperscriptBox["k", "s"]]}], "/;", " ", 
    RowBox[{
     RowBox[{"Re", "[", "s", "]"}], ">", "1"}]}]}]],
  TaggingRules:>"from www.functions.wolfram.com",ExpressionUUID->
  "f4415ec6-6b1d-4d8d-a206-321ed6676008"],
 "\n\nZeta[s]\[Equal]PolyLog[s,1] /;Re[s]>1\n\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{"Zeta", "[", "s", "]"}], "\[Equal]", 
   RowBox[{"-", 
    FractionBox[
     RowBox[{"PolyLog", "[", 
      RowBox[{"s", ",", 
       RowBox[{"-", "1"}]}], "]"}], 
     RowBox[{"1", "-", 
      SuperscriptBox["2", 
       RowBox[{"1", "-", "s"}]]}]]}]}]],
  TaggingRules:>"from www.functions.wolfram.com",ExpressionUUID->
