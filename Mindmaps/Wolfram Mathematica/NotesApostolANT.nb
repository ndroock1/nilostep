     RowBox[{"I", "(", "n", ")"}], " ", "=", " ", 
     RowBox[{
      RowBox[{"1", " ", "if", " ", "n"}], "=", "1"}]}], " ", ",", " ", 
    RowBox[{
     RowBox[{"I", "(", "n", ")"}], " ", "=", " ", 
     RowBox[{
      RowBox[{"0", " ", "if", " ", "n"}], "\[NotEqual]", "1.", " "}]}]}], 
   TraditionalForm]],ExpressionUUID->"9aabe2a7-de7a-4f33-ba06-cdf543876005"]
