Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"a", "(", "n", ")"}], " "}], TraditionalForm]],ExpressionUUID->
  "3221ba18-f048-4666-a629-757aea2f4354"],
 "be the characteristic function of the primes.\nThen ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"1", "<", "n", "\[LessEqual]", "x"}]], 
     RowBox[{"a", "(", "n", ")"}]}], "=", 
    RowBox[{"\[Pi]", "(", "x", ")"}], " "}], TraditionalForm]],ExpressionUUID->
  "f478d7d7-5e02-4550-a3d5-a1f1559a256e"],
 ", i.e. the prime-counting function.\nThen ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Theta]", "(", "x", ")"}], " ", "="}], TraditionalForm]],
  ExpressionUUID->"ba0f2c03-72be-4c55-b77a-87f77fb2a8e3"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["\[Sum]", 
     RowBox[{"p", "\[LessEqual]", "x"}]], 
    RowBox[{"Log", "(", "p", ")"}], " "}], TraditionalForm]],ExpressionUUID->
