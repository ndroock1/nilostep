Cell[TextData[{
 "The function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Pi]", 
    RowBox[{"(", "x", ")"}]}], TraditionalForm]], "InlineMath"],
 " (",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 "'s built-in function ",
 StyleBox["PrimePi[x]", "MR"],
 ") is a step function that jumps by 1 whenever ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]], "InlineMath"],
 " is prime. Riemann's prime-counting function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"R", 
    RowBox[{"(", "x", ")"}]}], TraditionalForm]], "InlineMath"],
 " (",
 StyleBox["Mathematica'",
  FontSlant->"Italic"],
 "s built-in function",
 StyleBox[" RiemannR[x]", "MR"],
 ") is defined as"
}], "DetailNotes",
 CellID->301210908],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   TagBox[
    RowBox[{
     TemplateBox[{"x"},
      "RiemannR"], "=", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"n", "=", "1"}], "\[Infinity]"], 
      RowBox[{
       FractionBox[
        RowBox[{
         TemplateBox[{"n"},
          "MoebiusMu"], " "}], "n"], 
       TemplateBox[{SuperscriptBox["x", 
          RowBox[{"1", "/", "n"}]]},
        "LogIntegral"]}]}]}],
    HoldForm], TraditionalForm]], "InlineMath"],
 ","
}], "DetailNotes",
 CellID->325710438],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"n"},
    "MoebiusMu"], TraditionalForm]], "InlineMath"],
 " is the M\[ODoubleDot]bius mu function \[Mu] (",
 StyleBox["MoebiusMu[n]", "MR"],
 " in ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ") and"
}], "DetailNotes",
 CellID->46506200],

